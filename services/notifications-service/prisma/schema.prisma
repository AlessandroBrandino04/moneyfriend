generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Notification {
  id        String   @id @default(cuid())
  userId    String?
  type      String
  payload   Json
  read      Boolean  @default(false)
  createdAt DateTime @default(now())
}
